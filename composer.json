{
    "name": "mondb-dev/x402-wp",
    "description": "WordPress plugin for x402 payment protocol paywalls",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin", "x402", "payment", "paywall", "blockchain", "crypto"],
    "homepage": "https://github.com/mondb-dev/x402-wp",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "mondb-dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "ext-json": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mondb-dev/x402-php.git"
        }
    ],
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
